<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="JCameraView">
        <attr name="easy_iconSize" format="dimension"/>
        <attr name="easy_iconMargin" format="dimension"/>
        <attr name="easy_iconSrc"  format="dimension"/>
        <attr name="easy_iconLeft" format="reference"/>
        <attr name="easy_iconRight" format="reference"/>
        <attr name="easy_duration_max" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ColorRadioButton">
        <attr name="radio_color" format="color"/>
        <attr name="radio_stroke_color" format="color"/>
    </declare-styleable>


    <attr name="image_gallery_span_count" format="integer" />
    <attr name="image_gallery_select_shade" format="color" />

    <declare-styleable name="IMGColorRadio" tools:ignore="ResourceName">

        <attr name="image_color" format="color" />
        <attr name="image_stroke_color" format="color" />

    </declare-styleable>


</resources>